generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  Admin
  SuperAdmin
}

model CoreUser {
  id       Int      @id @default(autoincrement())
  name     String
  password String
  createAt DateTime
  updateAt DateTime @updatedAt
}

model Booking {
  id Int @id @default(autoincrement())
}

model Organizations {
  id Int @id @default(autoincrement())
}

model Farmers {
  id Int @id @default(autoincrement())
}
